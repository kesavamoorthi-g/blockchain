{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "new-cap": ["error", { "capIsNew": false }],
      "object-curly-spacing": ["error","always"],
      "max-len": ["error", { "code": 1000 }]
  }
}
